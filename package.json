{
  "name": "chatgpt-web-service",
  "version": "1.0.0",
  "private": false,
  "description": "ChatGPT Web Service",
  "author": "AI-LAB",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": "^16 || ^18 || ^19"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "npm run migrate:up && NODE_ENV=devlopment esno watch ./src/index.ts",
    "prod": "node ./build/index.mjs",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
    "migrate:up": "node ./src/scripts/migrate.js",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:new": "sequelize-cli migration:generate --name 'migration'"
  },
  "dependencies": {
    "@alicloud/facebody20191230": "^4.0.8",
    "@alicloud/imageaudit20191230": "^2.0.5",
    "@alicloud/openapi-client": "^0.4.6",
    "@alicloud/tea-util": "^1.4.7",
    "archiver": "^5.3.1",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "chatgpt": "^5.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.0.3",
    "esno": "^0.16.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "https-proxy-agent": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "node-fetch": "^3.3.0",
    "node-stream-zip": "^1.15.0",
    "nodemailer": "^6.9.8",
    "qiniu": "^7.8.0",
    "sequelize": "^6.32.0",
    "sharp": "^0.32.6",
    "socks-proxy-agent": "^7.0.0",
    "string-random": "^0.1.3",
    "tencentcloud-sdk-nodejs": "^4.0.941",
    "uuid": "^10.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/archiver": "^5.3.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.35.0",
    "rimraf": "^4.3.0",
    "sequelize-cli": "6.6.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
